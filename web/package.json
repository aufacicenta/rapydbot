{
  "name": "@tgi/web",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aufacicenta/tgi.git"
  },
  "scripts": {
    "dev": "next dev -p 3003",
    "start:debug": "NODE_OPTIONS='--inspect=9229' next dev -p 3004 & npm run scss:class-types:watch",
    "build": "next build",
    "start": "next start",
    "scss:class-types": "typed-scss-modules 'src/**/**/*.scss' --ignore 'src/theme/*.scss' --exportType default --nameFormat none --implementation sass src --logLevel silent",
    "scss:class-types:watch": "npm run scss:class-types -- --watch",
    "plop": "plop",
    "stylelint": "stylelint",
    "api:codegen": "npm run api:codegen:dry",
    "api:codegen:dry": "graphql-codegen --config ./codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@apollo/server": "^4.4.1",
    "@as-integrations/next": "^1.2.0",
    "@popperjs/core": "^2.11.6",
    "ag-grid-community": "^29.1.0",
    "ag-grid-react": "^29.1.0",
    "clsx": "^1.2.1",
    "final-form": "^4.20.9",
    "final-form-arrays": "^3.1.0",
    "graphql": "^16.6.0",
    "i18next": "^22.4.11",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.13.0",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "^2.29.4",
    "next": "13.2.3",
    "next-i18next": "^13.2.1",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-final-form-listeners": "^1.0.3",
    "react-grid-system": "^8.1.9",
    "react-i18next": "^12.2.0",
    "react-popper": "^2.3.0",
    "react-transition-group": "^4.4.5",
    "rxjs": "^7.8.0",
    "sass": "^1.58.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/schema-ast": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^3.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mapbox-gl": "^2.7.10",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-final-form-listeners": "^1.0.0",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "inquirer-directory": "^2.2.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.2",
    "node-sass": "^8.0.0",
    "plop": "^3.1.2",
    "prettier": "^2.4.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^4.3.0",
    "typed-scss-modules": "^7.0.2",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "./{src,cypress,storybook,scripts}/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --list-different --write"
    ],
    "./**/*.{json,scss}": [
      "prettier --list-different --write"
    ],
    "./src/**/*.module.scss": [
      "stylelint --config ./app/.stylelintrc --fix",
      "typed-scss-modules --ignore 'src/theme/*.scss' --exportType default --nameFormat none --implementation node-sass --aliasPrefixes./src src",
      "prettier --list-different --write",
      "git add **/*.module.scss.d.ts"
    ]
  }
}
