{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "BOT: Attach by Process ID",
      "cwd": "${workspaceRoot}/bot/src",
      "trace": true,
      "port": 9228,
      "skipFiles": [
        "${workspaceRoot}/node_modules/**/*",
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "USER: Attach by Process ID",
      "cwd": "${workspaceRoot}/user/src",
      "trace": true,
      "port": 9226,
      "skipFiles": [
        "${workspaceRoot}/node_modules/**/*",
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "ORDER: Attach by Process ID",
      "cwd": "${workspaceRoot}/order/src",
      "trace": true,
      "port": 9227,
      "skipFiles": [
        "${workspaceRoot}/node_modules/**/*",
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "PRICE: Attach by Process ID",
      "cwd": "${workspaceRoot}/price/src",
      "trace": true,
      "port": 9225,
      "skipFiles": [
        "${workspaceRoot}/node_modules/**/*",
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "BOT: Jest Current File",
      "program": "${workspaceFolder}/bot/node_modules/.bin/jest",
      "cwd": "${workspaceFolder}/bot",
      "args": [
        "--config",
        "${workspaceFolder}/bot/jest.config.js",
        "--verbose",
        "--runInBand",
        "${fileBasenameNoExtension}"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "windows": {
        "program": "${workspaceFolder}/bot/node_modules/jest/bin/jest"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "USER: Jest Current File",
      "program": "${workspaceFolder}/user/node_modules/.bin/jest",
      "cwd": "${workspaceFolder}/user",
      "args": [
        "--config",
        "${workspaceFolder}/user/jest.config.js",
        "--verbose",
        "--runInBand",
        "${fileBasenameNoExtension}"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "windows": {
        "program": "${workspaceFolder}/user/node_modules/jest/bin/jest"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ORDER: Jest Current File",
      "program": "${workspaceFolder}/order/node_modules/.bin/jest",
      "cwd": "${workspaceFolder}/order",
      "args": [
        "--config",
        "${workspaceFolder}/order/jest.config.js",
        "--verbose",
        "--runInBand",
        "${fileBasenameNoExtension}"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "windows": {
        "program": "${workspaceFolder}/order/node_modules/jest/bin/jest"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "KYC: Jest Current File",
      "program": "${workspaceFolder}/kyc/node_modules/.bin/jest",
      "cwd": "${workspaceFolder}/kyc",
      "args": [
        "--config",
        "${workspaceFolder}/kyc/jest.config.js",
        "--verbose",
        "--runInBand",
        "${fileBasenameNoExtension}"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "windows": {
        "program": "${workspaceFolder}/kyc/node_modules/jest/bin/jest"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "PRICE: Jest Current File",
      "program": "${workspaceFolder}/price/node_modules/.bin/jest",
      "cwd": "${workspaceFolder}/price",
      "args": [
        "--config",
        "${workspaceFolder}/price/jest.config.js",
        "--verbose",
        "--runInBand",
        "${fileBasenameNoExtension}"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "windows": {
        "program": "${workspaceFolder}/price/node_modules/jest/bin/jest"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "PRICE: Jest Current Selected Test In File",
      "program": "${workspaceFolder}/price/node_modules/.bin/jest",
      "cwd": "${workspaceFolder}/price",
      "args": [
        "--config",
        "${workspaceFolder}/price/package.json",
        "--verbose",
        "--runInBand",
        "${fileBasenameNoExtension}",
        "-t",
        "${selectedText}"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "windows": {
        "program": "${workspaceFolder}/price/node_modules/jest/bin/jest"
      }
    }
  ]
}
